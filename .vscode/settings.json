{
  "files.exclude": {
		"node_modules": true,
		".vscode": true,
		".github": true,
		".commitizen.js": true,
		".commitlint.js": true,
		".editorconfig": true,
		".gitignore": true,
		"CODEOWNERS": true,
		"LICENSE": true,
		"package-lock.json": true,
		"package.json": true,
		"README.md": true
	},
  "npm-scripts.showStartNotification": false,
  "workspace.isHidden": false,
  "workspace.disableCounter": true,
  "workspace.disableColoring": false,
  "workspace.alwaysShowToggle": true,
  "workspace.statusbarPriority": 0
}
