{
  "name": "@sassify/type-of",
  "version": "1.0.1",
  "description": "Набор утилит (функций на языке CSS-препроцессора Sass) для определения типов данных",
  "main": "index.scss",
  "scripts": {
    "start": "npm run serve",
    "serve": "sass tests/index.test.scss:tests/index.test.css --no-source-map --watch && npm run docs",
    "test": "sass tests/index.test.scss:tests/index.test.css --no-source-map",
    "test-with-docs": "sass tests/index.test.scss:tests/index.test.css --no-source-map && npm run docs",
    "publish": "npm publish --access public",
    "docs": "npx sassdoc . -d docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sassify/type-of.git"
  },
  "keywords": [
    "therteenten",
    "sassify",
    "sass",
    "sass-mixins",
    "sass-functions",
    "sass-library",
    "scss",
    "scss-mixins",
    "scss-functions",
    "scss-library",
    "dart-sass"
  ],
  "author": "Haba Kudzaev (therteenten)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sassify/type-of/issues"
  },
  "homepage": "https://github.com/sassify/type-of#readme",
  "dependencies": {
    "sass": "^1.71.1"
  }
}
